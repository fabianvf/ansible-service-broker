---
registry:
  name: dockerhub
  url: xxx
  user: example
  pass: examplepass
  org: ansibleplaybookbundle
dao:
  etcd_host: etcd
  etcd_port: 2379
log:
  logfile: /var/log/ansible-service-broker/asb.log
  stdout: true
  level: debug
  color: true
openshift: {}
broker:
  dev_broker: true
  launch_apb_on_bind: false
  recovery: true
  output_request: true
